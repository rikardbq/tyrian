    "webpack": "^5.24.2"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/glob": "^7.1.3",
    "@types/node": "^12.19.9",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "node-html-parser": "^2.0.2"
