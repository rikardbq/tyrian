{
  "name": "tyrian",
  "version": "9.0.0-beta.1",
  "repository": "github:wizawu/tyrian",
  "license": "MIT",
  "bin": "bin/tyrian",
  "scripts": {
    "antlr": "cd src/grammar && antlr4 -Dlanguage=JavaScript -visitor Javap.g4",
    "build": "tsc -d --outDir dist -p .",
    "watch": "tsc -d --outDir dist -p . -w",
    "types": "eslint --ext .ts --fix @types",
    "lint": "eslint --ext .ts --ignore-pattern '/src/grammar/*' --fix src",
    "test": "mocha -V"
  },
  "dependencies": {
    "antlr4": "^4.9.0",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "jszip": "^3.6.0",
    "redent": "^3.0.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/antlr4": "^4.7.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^12.19.9",
    "@types/redent": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "node-html-parser": "^2.0.2"
  }
}
